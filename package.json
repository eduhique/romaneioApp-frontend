{
  "name": "romaneio-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run fix && ng serve --disable-host-check",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint --max-warnings 0",
